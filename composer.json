{
    "name": "aurigma/php-backoffice-client",
    "description": "PHP API Client for BackOffice API service of Customer's Canvas web-to-print system.",
    "keywords": [
        "customers canvas",
        "web-to-print",
        "api client",
        "backoffice"
    ],
    "homepage": "https://customerscanvas.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Aurigma Inc",
            "email": "info@aurigma.com",
            "homepage": "https://customerscanvas.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Aurigma\\BackOffice\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Aurigma\\BackOffice\\Test\\" : "test/" }
    }
}
